{
  "name": "Srcery",
  "description": "A color scheme with clearly defined constrasting colors and a slightly earthy tone. See more at https://srcery.sh",
  "author": "milogert",
  "version": "v0.3",
  "manifest_version": 4,
  "target": "System-Wide",
  "inject": {
    "shared.css": ["All"],
    "home-page.css": ["All"],
    "system.css": ["SP"],
    "notifications.css": ["All"],
    "toggle-switch.css": ["All"],
    "pill-menus.css": ["SP"],
    "friends-list.css": ["SP"],
    "downloads.css": ["SP"],
    "library.css": ["SP"],
    "Custom.css": ["SP"],
    "account-page.css": ["SP"]
  },
 "dependencies": {},
  "patches": {
    "Font": {
      "type": "checkbox",
      "values": {
        "Yes": { "font.css": ["All"] },
        "No": {}
      }
    },
    "Keyboard": {
      "type": "dropdown",
      "default": "Colorful (github.com/milogert)",
      "values": {
        "Colorful (github.com/milogert)": {
          "keyboards/milogert.css": ["All"]
        },
        "Muted (github.com/Tormak9970)": {
          "keyboards/tormak.css": ["All"]
        },
        "None": {}
      },
      "components": []
    },
    "Fullscreen Modals": {
      "type": "checkbox",
      "values": {
        "Yes": { "fullscreen-modals.css": ["SP"] },
        "No": {}
      }
    },
    "Quick Access": {
      "type": "checkbox",
      "values": {
        "Yes": { "quick-access-menu.css": ["QuickAccess"] },
        "No": {}
      }
    },
    "Main Menu": {
      "type": "checkbox",
      "values": {
        "Yes": { "main-menu.css": ["MainMenu"] },
        "No": {}
      }
    },
    "Settings": {
      "type": "checkbox",
      "values": {
        "Yes": { "settings.css": ["SP"] },
        "No": {}
      }
    },
    "Components": {
      "type": "checkbox",
      "values": {
        "Yes": { "Components.css": ["SP"] },
        "No": {}
      }
    },
    "Controller Settings": {
      "type": "checkbox",
      "values": {
        "Yes": { "controller-settings.css": ["SP", "MainMenu"] },
        "No": {}
      }
    },
    "In-Game Overlay": {
      "type": "checkbox",
      "values": {
        "Yes": { "in-game-overlay.css": ["MainMenu"] },
        "No": {}
      }
    }
  }
}
